.container
  .row
    .col.s3
      %button.btn{"ng-click": "$ctrl.add_log_entry('Some entry text')"} Add Entry
      %button.btn{"ng-click": "$ctrl.add_log_entry('Breathe', 'pyridostigmine')"} Add Breathe Entry
      %h2 Fewd
      %button.btn{"ng-click": "$ctrl.add_log_entry('Taco Tuesday', 'Taco Tuesday')"} Taco Tuesday!
      %br
      %button.btn{"ng-click": "$ctrl.add_log_entry('Soda: 20oz Bottle', 'Soda: 20oz Bottle')"} Soda: 20oz Bottle
      .h4.pink.lighten-5{style: "font-size: 3em", "am-time-ago": "$ctrl.latest_entry_by_entry_type[$ctrl.log_entry_type_id_by_name['Soda: 20oz Bottle']].created_at"}
      %br
      %button.btn{"ng-click": "$ctrl.add_log_entry('Soda: 12oz Can', 'Soda: 12oz Can')"} Soda: 12oz Can
      .h4.pink.lighten-5{style: "font-size: 3em", "am-time-ago": "$ctrl.latest_entry_by_entry_type[$ctrl.log_entry_type_id_by_name['Soda: 12oz Can']].created_at"}
    .col.s3
      %h2 Domains
      %ul
        %li{"ng-repeat": "domain in $ctrl.domains"}
          = "{{ domain.id }})"
          = "{{ domain.name }}"
      %h2 Users
      %ul
        %li{"ng-repeat": "user in $ctrl.users"}
          = "{{ user.user_id }})"
          = "{{ user.name }}"
    .col.s3
      %h2 Entry Types
      %ul
        %li{"ng-repeat": "log_entry_type in $ctrl.log_entry_types"}
          = "{{ log_entry_type.id }})"
          = "{{ log_entry_type.value }}"
